{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/youtube-ig-webapp/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
    {
        "src": "/api/(.*)",
        "dest": "/api/index.py"
    },
    {
        "src": "/assets/(.*)",
        "dest": "/frontend/youtube-ig-webapp/dist/assets/$1"
    },
    {
        "src": "/(.*)",
        "dest": "/frontend/youtube-ig-webapp/dist/index.html"
    }
    ]
  }